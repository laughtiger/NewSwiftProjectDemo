import Foundation
extension NSData {
    /**
      返回当前时间 2000-01-01
     */
    class var currentTime: String {
        let formater = NSDateFormatter.init()
        formater.dateFormat = "yyyy-MM-dd"
        let curDate = NSDate()
        return formater.stringFromDate(curDate)
    }
}
